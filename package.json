{
  "name": "kprotractor",
  "version": "1.0.0",
  "description": "protractor cucumber typescript jasimine chai for angular/non angular apps with protractor-multiple-cucumber-html framework",
  "keywords": [
    "protractor",
    "cucumber",
    "chai",
    "angular",
    "angularjs",
    "testing",
    "behaviour driven development",
    "bdd",
    "selenium",
    "webdriverJS",
    "gherkin",
    "automation testing"
  ],
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist/out-tsc/",
    "clean-build": "npm run clean && npm run build",
