    "cz-conventional-changelog": "^2.0.0",
    "emailjs-imap-client": "3.0.5",
    "electron": "1.7.6",
    "htmlhint": "^0.9.13",
    "husky": "^0.14.3",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-istanbul-threshold": "^1.2.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "mail-listener2": "^0.3.1",
    "npm-run-all": "^4.1.2",
    "protractor-cucumber-framework": "^5.0.0",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.7.0",
    "rxjs-tslint-rules": "^3.17.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "^6.0.0",
    "tslint": "^5.8.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "~2.6.2",
    "validate-commit-msg": "^2.14.0",
    "wallaby-webpack": "^3.9.3",
    "webpack-bundle-analyzer": "^2.11.1"
  },
  "engines": {
    "node": ">=6.9.x",
    "npm": ">=5.x",
    "yarn": ">=1.3.x"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
