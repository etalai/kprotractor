{
  "name": "ng-sparta",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "analyse:cloc": "cloc src",
    "analyse:cloc:user": "git ls-files | while read f; do git blame --line-porcelain $f | grep '^author '; done | sort -f | uniq -ic | sort -n",
    "analyse:similar-code": "synt analyze src",
    "analyse:stats": "webpack-bundle-analyzer dist/stats.json",
    "analyse:stats:build": "npm run build -- --stats-json && npm run analyse:stats",
    "analyse:stats:build:prod": "npm run build:prod -- --stats-json && npm run analyse:stats",
    "build": "ng build",
    "build:dev": "ng build --dev",
    "build:prod": "ng build --prod",
    "build:qa": "ng build --qa",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "ci": "npm-run-all ci:*",
    "ci:lint": "npm run lint",
    "ci:test": "ng test --code-coverage --watch=false --single-run=true --browsers=ChromeHeadlessNoSandbox --reporters=progress,junit,coverage-istanbul,istanbul-threshold",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "cucumber": "protractor protractor-cucumber.conf.js",
    "cucumber:local": "npm run cucumber -- --baseUrl http://app.qa.datarepublic.com.au",
    "doc:generate": "./node_modules/.bin/compodoc -p tsconfig.json",
    "doc:serve": "./node_modules/.bin/compodoc -s",
    "e2e": "ng e2e",
    "lint": "npm-run-all lint:*",
    "lint:html": "htmlhint --config .htmlhintrc src",
    "lint:ts": "ng lint",
    "ng": "ng",
    "precommit": "npm run ci:lint",
    "precucumber": "webdriver-manager update",
    "postmerge": "yarn",
    "start": "ng serve --host local.dev.datarepublic.com.au --port 8000 --base-href /",
    "serve:dist": "live-server dist --host=local.dev.datarepublic.com.au --port=8000",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:vscode-debug": "ng test --browsers ChromeDebugging"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/cdk": "^5.2.5",
    "@types/chai": "^4.1.3",
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@types/cucumber": "^4.0.4",
    "@angular/flex-layout": "^2.0.0-beta.12",
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@angular/material": "^5.2.5",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/platform-server": "^5.2.10",
    "@angular/router": "^5.2.10",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/notify": "julienevano/notify#chore/upgrade-dependencies",
    "@ngrx/router-store": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "@ngrx/store-devtools": "^4.1.1",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "@swimlane/ngx-charts": "^7.3.0",
    "angular-linky": "^1.2.2",
    "angular2-autosize": "^1.0.1",
    "angular2-cookie": "^1.2.6",
    "brace": "^0.11.1",
    "chai": "^4.1.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.5",
    "cucumber": "^4.2.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "i18n-iso-countries": "^3.6.2",
    "intl": "^1.2.5",
    "lodash": "^4.17.5",
    "md5": "^2.2.1",
    "moment": "^2.22.1",
    "ng-socket-io": "^0.2.0",
    "ng2-ace-editor": "^0.3.1",
    "ng2-file-upload": "1.3.0",
    "ng2-page-scroll": "^4.0.0-beta.11",
    "ng2-validation": "^4.2.0",
    "ng2-webstorage": "^2.0.0",
    "ng2letteravatar": "julienevano/ng2letteravatar",
    "ngrx-store-freeze": "^0.2.2",
    "ngx-clipboard": "^9.0.0",
    "ngx-infinite-scroll": "^0.8.4",
    "ngx-moment": "^2.0.0-rc.0",
